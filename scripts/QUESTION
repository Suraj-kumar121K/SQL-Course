--> ALL TABLE SHOW
SELECT * FROM EMP

1  --> Write A Query To Display(WAQTD) THE ANNUAL SALARY OF THE EMPLOYEE WHOSE NAME IS SMITH.
 SELECT ENAME, SAL*12 FROM EMP
 WHERE ENAME='SMITH'

2  --> WAQTD NAME OF THE EMPLOYEES WORKING AS CLERK
SELECT ENAME, JOB FROM EMP
WHERE JOB='CLERK'

3  --> WAQTD SALARY OF THE EMPLOYEES WHO ARE WORKING AS SALESMAN
SELECT ENAME, 
       SAL, 
       JOB 
FROM EMP
WHERE JOB = 'SALESMAN'

4  --> WAQTD DETAILS OF THE EMP WHO EARNS MORE THAN 2000
SELECT * FROM EMP
WHERE SAL > 2000

5  --> WAQTD DETAILS OF THE EMP WHOSE NAME IS JONES
SELECT EMPNO,
       ENAME,
       JOB,
       SAL,
       DEPTNO
FROM EMP
WHERE SAL > 2000;

6  --> WAQTD DETAILS OF THE EMP WHO WAS HIRED AFTER 01-JAN-81
SELECT EMPNO,
       ENAME,
       JOB,
       SAL,
       DEPTNO
FROM EMP
WHERE ENAME = 'JONES';

7  --> WAQTD NAME AND SAL ALONG WITH HIS ANNUAL SALARY IF THE ANNUAL SALARY IS MORE THAN 12000
SELECT * 
FROM EMP
WHERE ENAME = 'JONES'

8  --> WAQTD EMPNO OF THE EMPLOYEES WHO ARE WORKING IN DEPT 30
SELECT *
FROM EMP
WHERE HIREDATE > '01-JAN-1982'

9  --> WAQTD ENAME AND HIREDATE IF THEY ARE HIRED BEFORE 1981
SELECT ENAME,
       SAL,
       SAL * 12 AS ANNUAL_SALARY
FROM EMP
WHERE SAL * 12 > 12000

10 --> WAQTD DETAILS OF THE EMPLOYEES WORKING AS MANAGER
SELECT * 
FROM EMP
WHERE JOB = 'MANAGER'

11 --> WAQTD NAME AND SALARY GIVEN TO AN EMPLOYEE IF EMPLOYEEE EARNS A COMMISSION OF RUPEES 1400
SELECT ENAME AS EMPLOYEE,
       SAL AS SALARY
FROM EMP
WHERE COMM = 1400

12 --> WAQTD DETAILS OF EMPLOYEES HAVING COMMISSION MORE THAN SALARY
SELECT * 
FROM EMP
WHERE COMM > SAL

13 --> WAQTD EMPNO OF EMPLOYEES HIRED BEFORE THE YEAR 87
SELECT * 
FROM EMP
WHERE YEAR(HIREDATE) < 1987

14 --> WAQTD DETAILS OF EMPLOYEES WORKING AS AN N ANALYST
SELECT * 
FROM EMP
WHERE JOB = 'ANALYST';

15 --> WAQTD DETAILS OF EMPS EARNING MORE THAN 2000 RUPEES PER MONTH
SELECT * 
FROM EMP
WHERE SAL > 2000;

